package multithreading;

class Hi extends Thread{
	
	public void run(){
		for (int i = 0; i < 4; i++) {
			try {
				Thread.sleep(5);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}
}

